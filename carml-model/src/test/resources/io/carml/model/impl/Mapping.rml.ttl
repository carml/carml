@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix carml: <http://carml.taxonic.com/carml/> .
@prefix ex: <http://example.org/> .
@prefix fn: <http://example.org/function/> .
@prefix d2rq : <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#> .

ex:TriplesMap a rr:TriplesMap ;
  rml:logicalSource [
    rml:source "some-source.xml" ;
    rml:iterator "/*" ;
    rml:referenceFormulation ql:XPath ;
  ] ;
  rr:subjectMap [
    rr:template "http://example.org/{/some-node}" ;
    rr:class
      ex:Class1 , ex:Class2 , ex:Class3 ;
    rr:graph
      ex:Graph1, ex:Graph2 ;
    rr:graphMap [
        rml:reference "graph" ;
        rr:termType rr:IRI ;
    ] ;
  ] ;
  rr:subjectMap [
    rr:template "http://example.org/{/some-other-node}" ;
    rr:class
      ex:Class4 , ex:Class5 ;
    rr:graph ex:Graph3 ;
    rr:graphMap [
        rml:reference "graph3" ;
        rr:termType rr:BlankNode ;
    ] ;
  ] ;
  rr:predicateObjectMap [
    rr:predicate
      ex:predicate1 ,
      ex:predicate2 ;
    rr:objectMap
      [
        rml:reference "/some-value" ;
        rr:language "en" ;
      ] ;
      rr:objectMap [
        rr:template "some-template{/some-value}" ;
        rr:datatype ex:SomeDatatype ;
      ] ;
      rr:objectMap [
        fnml:functionValue [
          rml:logicalSource [] ;
          rr:subjectMap [
            rr:template "functionExec" ;
            rr:termType rr:BlankNode ;
            rr:class fno:Execution ;
          ] ;
          rr:predicateObjectMap
            [
              rr:predicate fno:executes ;
              rr:object ex:sumFunction ;
            ] ,
            [
              rr:predicate ex:intParameterA ;
              rr:objectMap [ rml:reference "foo" ]
            ] ,
            [
              rr:predicate ex:intParameterB  ;
              rr:objectMap [ rml:reference "bar" ]
            ]
        ] ;
        rr:datatype xsd:integer ;
      ] ;
  ] ;
.

ex:FileSourceTM a rr:TriplesMap ;
  rml:logicalSource [
    rml:source [
      a carml:Stream ;
      carml:declaresNamespace [
        carml:namespacePrefix "ex" ;
        carml:namespaceName "http://www.example.com/books/1.0/" ;
      ] ;
      carml:streamName "bla" ;
      carml:url "file:///some/file"
    ] ;
    rml:iterator "/*" ;
    rml:referenceFormulation ql:XPath ;
  ] ;
  rr:subject ex:subject ;
  rr:class ex:ClassFileSource ;
  rr:predicateObjectMap [
    rr:predicateMap [
      rr:template "https://{/pred}" ;
    ] ;
    rr:objectMap ex:refObjectMap ;
  ] ;
.

ex:refObjectMap a rr:ObjectMap ;
  rr:parentTriplesMap ex:TriplesMap ;
  rr:joinCondition
    [
      rr:child "child1" ;
      rr:parent "parent1" ;
    ] ,
    [
      rr:child "child2" ;
      rr:parent "parent2" ;
    ] ;
.

ex:SimpleExecutionMapping
  rr:template "functionExec";
  rr:termType rr:BlankNode ;
  rr:class fno:Execution ;
.

ex:FunctionTM a rr:TriplesMap ;
  rr:subjectMap [
    fnml:functionValue [
      rr:subjectMap ex:SimpleExecutionMapping ;
      rr:predicateObjectMap
        [
          rr:predicate fno:executes ;
          rr:object fn:concat ;
        ] ,
        [
          rr:predicate fn:valueParameter ;
          rr:object "http://example.org/" ;
        ] ,
        [
          rr:predicate fn:otherValueParameter ;
          rr:objectMap [
            rml:reference "Name" ;
          ] ;
        ] ;
    ] ;
  ] ;
.

ex:TriplesMap2 a rr:TriplesMap ;
  rr:logicalTable [
    rr:tableName "some_table" ;
  ] ;
  rr:subjectMap [
    rr:template "http://example.org/{/some-node}" ;
    rr:class ex:Class4 ;
  ] ;
.

ex:TriplesMap3 a rr:TriplesMap ;
  rr:logicalTable [
    rr:sqlQuery "select * from some_table" ;
    rr:sqlVersion rr:SQL2008 ;
  ] ;
  rr:subjectMap [
    rr:template "http://example.org/{/some-node}" ;
    rr:class ex:Class5 ;
  ] ;
.

ex:TriplesMap4 a rr:TriplesMap ;
  rml:logicalSource [
    rml:source [
      a d2rq:Database ;
      d2rq:jdbcDSN "jdbc:mysql://localhost/example" ;
      d2rq:jdbcDriver "com.mysql.jdbc.Driver" ;
      d2rq:username "user" ;
      d2rq:password "password" ;
      rml:query "select * from some_table" ;
    ] ;
    rml:referenceFormulation ql:Rdb ;
    rr:sqlVersion rr:SQL2008 ;
  ] ;
  rr:subjectMap [
    rr:template "http://example.org/{/some-node}" ;
    rr:class ex:Class6 ;
  ] ;
.

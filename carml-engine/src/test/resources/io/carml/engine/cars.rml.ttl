@prefix rml: <http://w3id.org/rml/> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix exm: <http://example.com/mapping/> .
@prefix data: <http://example.com/data/> .
@prefix car: <http://example.com/car/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix carml: <http://carml.taxonic.com/carml/> .

exm:LogicalSource a rml:LogicalSource ;
  #rml:source "cars.csv" ;
  rml:source [
    a carml:Stream ;
    rml:null "" ;
  ] ;
  rml:referenceFormulation ql:CSV ;
.

exm:CarMapping a rml:TriplesMap ;
  rml:logicalSource exm:LogicalSource ;
  rml:subjectMap [
    rml:template "http://example.com/data/car/{Make}-{Model}" ;
    rml:class car:Car ;
  ] ;
  rml:predicateObjectMap [
   rml:predicate car:make ;
   rml:objectMap [
     rml:parentTriplesMap exm:MakeMapping ;
   ] ;
  ] ;
  rml:predicateObjectMap [
    rml:predicate dct:description ;
    rml:objectMap [
      rml:reference "Description" ;
    ] ;
  ] ;
  rml:predicateObjectMap [
    rml:predicate car:year ;
    rml:objectMap [
      rml:template "http://example.com/data/year/{Year}" ;
    ] ;
  ] ;
  rml:predicateObjectMap [
    rml:predicate car:price ;
    rml:objectMap [
      rml:reference "Price" ;
    ] ;
  ] ;
.

exm:MakeMapping a rml:TriplesMap ;
  rml:logicalSource exm:LogicalSource ;
  rml:subjectMap [
    rml:template "http://example.com/data/make/{Make}" ;
    rml:class car:CarMake ;
  ] ;
.

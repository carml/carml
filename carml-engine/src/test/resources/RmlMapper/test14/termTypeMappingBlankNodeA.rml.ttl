@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ex: <http://example.com/>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<#TermTypeMappingBlankNodeA> a rr:TriplesMap;
	rml:logicalSource [
		rml:source "simple2TestInput.json";
		rml:referenceFormulation ql:JSONPath;
		rml:iterator "$.Child[*]"
	];


	rr:subjectMap [
		rr:template "http://example.com/Child/{first}/{last}";
		rr:class ex:Child;
	];

	rr:predicateObjectMap [
		rr:predicateMap [
			rr:constant ex:hasBirthday;
		];
		rr:objectMap [
			rr:template "http://example.com/ThisHasToBeABlankNode/{birthday}";
			rr:termType rr:BlankNode
		]
	] .
